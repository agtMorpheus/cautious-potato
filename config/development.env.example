# Contract Manager Configuration (Development)
# Copy this file to development.env and configure for your local environment
# DO NOT commit development.env with real credentials

# Database Configuration
DB_HOST=localhost
DB_NAME=contract_manager_dev
DB_USER=root
DB_PASS=

# Application Settings
APP_ENV=development
APP_DEBUG=true
SESSION_TIMEOUT=7200

# Session Configuration
SESSION_SECURE_COOKIE=false
SESSION_SAMESITE=Lax

# Logging
LOG_LEVEL=debug
LOG_FILE=logs/app.log

# CORS Configuration - Updated for development with credentials support
CORS_ORIGIN=http://localhost:3000,http://127.0.0.1:3000,http://localhost:8000,http://127.0.0.1:8000

# Security
CSRF_TOKEN_ENABLED=false

# Rate Limiting
RATE_LIMIT=1000
