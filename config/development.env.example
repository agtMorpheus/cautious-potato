# Contract Manager Configuration (Phase 5)
# Copy this file to production.env and configure for your environment
# DO NOT commit production.env with real credentials

# Database Configuration
DB_HOST=localhost
DB_NAME=contract_manager
DB_USER=root
DB_PASS=

# Application Settings
APP_ENV=development
APP_DEBUG=true
SESSION_TIMEOUT=3600

# Logging
LOG_LEVEL=debug
LOG_FILE=/var/log/contract_manager.log

# CORS (set to your frontend domain in production)
CORS_ORIGIN=*

# Security
CSRF_TOKEN_ENABLED=false
SESSION_SECURE_COOKIE=false

# Rate Limiting
RATE_LIMIT=60
